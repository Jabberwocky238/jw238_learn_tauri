{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "ddd",
    "version": "0.0.1"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": true,
        "execute": true,
        "sidecar": true,
        "open": true,
        "scope": [
          {
            "name": "conda2",
            "cmd": "conda",
            "args": [{ "validator": "\\S+" },{ "validator": "\\S+" }]
          },
          {
            "name": "conda3",
            "cmd": "conda",
            "args": [{ "validator": "\\S+" },{ "validator": "\\S+" },{ "validator": "\\S+" }]
          },
          {
            "name": "conda4",
            "cmd": "conda",
            "args": [{ "validator": "\\S+" },{ "validator": "\\S+" },{ "validator": "\\S+" },{ "validator": "\\S+" }]
          },
          {
            "name": "conda5",
            "cmd": "conda",
            "args": [{ "validator": "\\S+" },{ "validator": "\\S+" },{ "validator": "\\S+" },{ "validator": "\\S+" },{ "validator": "\\S+" }]
          },
          {
            "name": "where",
            "cmd": "where",
            "args": [{ "validator": "\\S+" }]
          }
        ]
      },
      "fs": {
        "all": true,
        "readFile": true,
        "writeFile": true,
        "readDir": true,
        "copyFile": true,
        "createDir": true,
        "removeDir": true,
        "removeFile": true,
        "renameFile": true,
        "exists": true,
        "scope": [
          "$APPCONFIG/*"
        ]
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.JW.dev",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "ddd",
        "width": 800,
        "height": 600
      }
    ]
  }
}